{
  "liver_replacement_journey": {
    "name": "Hip Replacement Recovery Journey",
    "start_node_id": "welcome",
    "nodes": [
      {
        "id": "welcome",
        "type": "MESSAGE",
        "message": "Welcome to your hip replacement recovery journey! We're here to support you every step of the way.",
        "next_node_id": "age_check"
      },
      {
        "id": "age_check",
        "type": "CONDITIONAL",
        "condition": {
          "field": "age",
          "operator": "gte",
          "value": 65
        },
        "on_true_next_node_id": "senior_program",
        "on_false_next_node_id": "standard_program"
      },
      {
        "id": "senior_program",
        "type": "MESSAGE",
        "message": "You've been enrolled in our Senior Care Program with additional support and monitoring.",
        "next_node_id": "language_check"
      },
      {
        "id": "standard_program",
        "type": "MESSAGE",
        "message": "You've been enrolled in our Standard Recovery Program.",
        "next_node_id": "language_check"
      },
      {
        "id": "language_check",
        "type": "CONDITIONAL",
        "condition": {
          "field": "language",
          "operator": "eq",
          "value": "es"
        },
        "on_true_next_node_id": "spanish_resources",
        "on_false_next_node_id": "english_resources"
      },
      {
        "id": "spanish_resources",
        "type": "MESSAGE",
        "message": "Hemos preparado recursos en español para apoyar su recuperación.",
        "next_node_id": "first_delay"
      },
      {
        "id": "english_resources",
        "type": "MESSAGE",
        "message": "We've prepared comprehensive resources to support your recovery.",
        "next_node_id": "first_delay"
      },
      {
        "id": "first_delay",
        "type": "DELAY",
        "duration_seconds": 10,
        "next_node_id": "day_1_followup"
      },
      {
        "id": "day_1_followup",
        "type": "MESSAGE",
        "message": "Day 1: How are you feeling? Remember to take your medication as prescribed and follow your mobility exercises.",
        "next_node_id": "week_1_delay"
      },
      {
        "id": "week_1_delay",
        "type": "DELAY",
        "duration_seconds": 30,
        "next_node_id": "week_1_followup"
      },
      {
        "id": "week_1_followup",
        "type": "MESSAGE",
        "message": "Week 1: Great progress! Continue with your physical therapy and let us know if you have any concerns.",
        "next_node_id": null
      }
    ]
  },
  "knee_replacement_journey": {
    "name": "Knee Replacement Recovery Journey",
    "start_node_id": "welcome_knee",
    "nodes": [
      {
        "id": "welcome_knee",
        "type": "MESSAGE",
        "message": "Welcome to your knee replacement recovery journey! Your dedication to recovery will make all the difference.",
        "next_node_id": "condition_specific_info"
      },
      {
        "id": "condition_specific_info",
        "type": "MESSAGE",
        "message": "Knee replacement recovery typically involves different exercises than hip replacement. We've customized your plan accordingly.",
        "next_node_id": "initial_delay"
      },
      {
        "id": "initial_delay",
        "type": "DELAY",
        "duration_seconds": 5,
        "next_node_id": "mobility_reminder"
      },
      {
        "id": "mobility_reminder",
        "type": "MESSAGE",
        "message": "Remember: Early mobility is key to successful knee replacement recovery. Start with gentle range-of-motion exercises as directed.",
        "next_node_id": null
      }
    ]
  },
  "simple_test_journey": {
    "name": "Simple Test Journey",
    "start_node_id": "start",
    "nodes": [
      {
        "id": "start",
        "type": "MESSAGE",
        "message": "Starting your test journey...",
        "next_node_id": "wait"
      },
      {
        "id": "wait",
        "type": "DELAY",
        "duration_seconds": 3,
        "next_node_id": "check_age"
      },
      {
        "id": "check_age",
        "type": "CONDITIONAL",
        "condition": {
          "field": "age",
          "operator": "gt",
          "value": 50
        },
        "on_true_next_node_id": "mature_message",
        "on_false_next_node_id": "young_message"
      },
      {
        "id": "mature_message",
        "type": "MESSAGE",
        "message": "You have the wisdom of experience!",
        "next_node_id": null
      },
      {
        "id": "young_message",
        "type": "MESSAGE",
        "message": "Youth brings energy and quick recovery!",
        "next_node_id": null
      }
    ]
  }
}